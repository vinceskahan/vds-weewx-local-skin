      function openURL(urlname)
      {
      window.location=urlname;
      }
    </script>

  </head>

  <body>
    <div id="container">
      <div id="masthead">
        <h1>$station.location</h1>
        <h2>Ten Year Weather Summary</h2>
          <h2>$current.dateTime</h2>
      </div>
      <div id="content">

      <div id="navbar">
<center>
